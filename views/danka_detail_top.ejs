              <%
                var result = resultRows[0];
                var memberId = result.member_id;
                var dankaType = result.danka_type;
                var nameSei = result.name_sei;
                var nameNa = result.name_na;
                var nameFull = nameSei + "　" + nameNa;
                var tikuCode = result.tiku_code;
                var sewaCode = result.member_id_sewa;
                var tikuName = tikuInfoJson[tikuCode];
                var sewaName = tikuInfoJson[sewaCode];
                var dankaTypeDanka = "○";
                var dankaTypeShinto = "-";
                if (dankaType === 1){
                    dankaTypeDanka = "-";
                    dankaTypeShinto = "○";
                }
                var sex = result.sex;
                var jobCode = result.job_code;
                var birthdayY = result.birthday_y;
                var birthdayM = result.birthday_m;
                var birthdayD = result.birthday_d;
                var memberIdSou = result.member_id_sou;
                var furiganaSei = result.furigana_sei;
                var furiganaNa = result.furigana_na;
              %>    
<script>
    $(function () {
        $(".menu_sub_kihon").click(function () {
            $("#danka_detail_kihon_form").submit();
        });
        $(".menu_sub_kako").click(function () {
            $("#danka_detail_kako_form").submit();
        });
        $(".menu_sub_genzai").click(function () {
            $("#danka_detail_genzai_form").submit();
        });
        $(".menu_sub_nenkaiki").click(function () {
            $("#danka_detail_nenkaiki_form").submit();
        });
        $(".menu_sub_kihu").click(function () {
            $("#danka_detail_kihu_form").submit();
        });
        $(".menu_sub_omairi").click(function () {
            $("#danka_detail_omairi_form").submit();
        });
    });
</script>
