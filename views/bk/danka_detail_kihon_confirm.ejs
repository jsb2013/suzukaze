                    </script>
                </tr>
                <tr>
                  <td id="email_type">Email(優先2)</td>
                    <script>
                      createHtml("<%= mailAddress2Bk %>", "<%= mailAddress2 %>");
                    </script>
                </tr>
                <tr>
                  <td id="email_type">Email(優先3)</td>
                    <script>
                      createHtml("<%= mailAddress3Bk %>", "<%= mailAddress3 %>");
                    </script>
                </tr>
                <tr>
