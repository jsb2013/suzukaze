                <tr>
                  <td id="danka_type">登録区分</td>
                  <script>
                      createHtmlByDankaType("<%= dankaTypeBk %>", "<%= dankaType %>");
                  </script>                    
                </tr>
                <tr>
                  <td id="name_type">戸主名(※必須)</td>
                    <script>
                      createHtml("<%= nameFullBk %>", "<%= nameFull %>");
                    </script>
                </tr>
                <tr>
                  <td id="furigana_type">ふりがな(※必須)</td>
                    <script>
                      createHtml("<%= furiganaFullBk %>", "<%= furiganaFull %>");
                    </script>
                </tr>
                <tr>
                  <td id="sex_type">性別(※必須)</td>
                    <script>
                      createHtmlBySex("<%= sexBk %>", "<%= sex %>");
                    </script>
                </tr>
                <tr>
                  <td id="zip_code_type">郵便番号</td>
                    <script>
                      createHtml("<%= zipCodeBk %>", "<%= zipCode %>");
                    </script>
                </tr>
                <tr>
                  <td id="region_type">都道府県</td>
                    <script>
                      createHtml("<%= regionBk %>", "<%= region %>");
                    </script>
                </tr>
                <tr>
                  <td id="city_type">市区町村</td>
                    <script>
                      createHtml("<%= cityBk %>", "<%= city %>");
                    </script>
                </tr>
                <tr>
                  <td id="address_line1_type">番地</td>
                    <script>
                      createHtml("<%= addressLine1Bk %>", "<%= addressLine1 %>");
                    </script>
                </tr>
                <tr>
