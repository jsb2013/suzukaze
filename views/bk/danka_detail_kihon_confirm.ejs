                  <td id="sou_type">担当者[僧]</td>
                    <script>
                      createHtml("<%= souNameBk %>", "<%= souName %>");
                    </script>
                </tr>
                <tr>
                  <td class="comment_type">備考<br>※500文字以内</td>
                    <script>
                      createHtml("<%= commentBk %>", "<%= comment %>");
                    </script>
                </tr>
