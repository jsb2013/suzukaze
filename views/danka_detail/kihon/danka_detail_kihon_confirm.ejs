            <input type="hidden" name="sewa_code" value=<%= sewaCode %>>
            <input type="hidden" name="tiku_code" value=<%= tikuCode %>>
            <input type="hidden" name="comment" value=<%= webItemJson.comment %>>
            <input type="hidden" name="tags" value=<%= tags %>>
            <input type="hidden" name="jiin" value=<%= jiinCode %>>
            <input type="hidden" name="tiku_code_bk" value=<%= tikuCodeBk %>>
            <input type="hidden" name="is_update_m_member" value=false>
            <input type="hidden" name="is_update_m_address" value=false>
            <input type="hidden" name="is_update_m_telnumber_1" value=false>
