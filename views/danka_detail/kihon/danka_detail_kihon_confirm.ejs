<script>
    $(function(){

    // 更新時のバリデーションチェック
        $("form").submit(function () {
            $(".is_change").each(function () {
                if ($(this).parent().hasClass("m_member")) {
                    $("input[name='is_update_m_member']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("t_danka")) {
                    $("input[name='is_update_t_danka']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_address")) {
                    $("input[name='is_update_m_address']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_telnumber_1")) {
                    $("input[name='is_update_m_telnumber_1']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_telnumber_2")) {
                    $("input[name='is_update_m_telnumber_2']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_telnumber_3")) {
                    $("input[name='is_update_m_telnumber_3']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_mail_1")) {
                    $("input[name='is_update_m_mail_1']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_mail_2")) {
                    $("input[name='is_update_m_mail_2']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("m_mail_3")) {
                    $("input[name='is_update_m_mail_3']").val(true);
                    //return false;
                }
                if ($(this).parent().hasClass("t_comment")) {
                    $("input[name='is_update_t_comment']").val(true);
                    //return false;
                }
            });
        });
    // ページ遷移定義
    $("#return").click(function () {
        $("#danka_detail_kihon_form").submit();
    });
    $(".menu_sub_kihon").click(function () {
        $("#danka_detail_kihon_form").submit();
    });
    $(".menu_sub_kako").click(function () {
        $("#danka_detail_kako_form").submit();
    });
    $(".menu_sub_genzai").click(function () {
        $("#danka_detail_genzai_form").submit();
    });
    $(".menu_sub_nenkaiki").click(function () {
        $("#danka_detail_nenkaiki_form").submit();
    });
    $(".menu_sub_kihu").click(function () {
        $("#danka_detail_kihu_form").submit();
    });
    $(".menu_sub_omairi").click(function () {
        $("#danka_detail_omairi_form").submit();
    });
    })
</script>
