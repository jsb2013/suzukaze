<body>
<!-- container -->
<div id="container">
  <!-- header -->
  <div id="header">
    <div id="headerMain">
      <h1>長伝寺様向けカスタマイズ</h1>
    </div>
    <div id="headerLogo">
      <img src="img/logo1_dannkasystem.gif" alt="logo1_dannkasystem">
    </div>
  </div>
  <!-- header end -->
  <!-- inner -->
  <div id="inner">
    <!-- menu_box -->
    <div id="menu_box">
      <div id="temple_img"><img src="img/tyo-denji.jpg" alt="temple-image" width="198" height="132" border=0></div>
      <!-- menu -->
      <div id="menu">
        <nav>
          <ul>
            <li><span class="ja">スケジュール</span><span class="en">schedule</span>
          </ul>
          <ul>
            <li><a href="/danka_top"><span class="ja">檀家管理</span><span class="en">danke-manage</span></a>  
            <li><span class="ja">僧・世話人管理</span><span class="en">so-manage</span>
            <li><span class="ja">台帳管理</span><span class="en">master</span>
            <li><a href="/report_top"><span class="ja">帳票出力</span><span class="en">out-tyohyo</span></a>
          </ul>
          <ul>
            <li><span class="ja">備品管理</span><span class="en">stock-manage</span>
            <li><span class="ja">寺院情報</span><span class="en">user-info</span>
          </ul>
        </nav>
      </div>
      <!-- menu end -->
    </div>
    <!-- menu_box end -->
    <!-- wrapper -->
    <div id="wrapper">
      <!-- inner_title -->
      <div class="inner_title">
        <div class="title">
   	      <h2><span class="ja">檀家管理詳細</span><span class="en">danka detail infomation</span></h2>
        </div>
        <div class="kosyu_info">
   	      <table>
              <thead>
                <tr>
                  <th class="kosyu_info_kosyu_th">戸主名</th>
                  <th class="kosyu_info_type_danka_th">檀家</th>
                  <th class="kosyu_info_type_shinto_th">信徒</th>
                  <th class="kosyu_info_tiku_th">地区</th>
                  <th class="kosyu_info_sewa_th">世話人</th>
                </tr>
              </thead>
              <tbody>
                <tr id="<%= memberId %>">
                  <td class="table_name_full"><%= nameFullBk %></td>
                  <td class="table_danka_type_danka"><%= dankaTypeDankaBk %></td>
                  <td class="table_danka_type_shinto"><%= dankaTypeShintoBk %></td>
                  <td id="<%= tikuCodeBk %>" class="table_tiku_name"><%= tikuNameBk %></td>
                  <td id="<%= sewaCodeBk %>" class="table_sewa_name"><%= sewaNameBk %></td>
                </tr>
              </tbody>
            </table>
        </div>
        <div class="menu_sub">
          <ul>
            <li class="menu_sub_kihon">基本情報 
            <li class="menu_sub_kako">過去帳
            <li class="menu_sub_nenkaiki_dummy">年回忌
          </ul>
        </div>
      </div>
      <!-- title end-->
      <!-- inner_main □□□□□□□□□□□□□□□□□□□□□□□□□□□□□□□□ -->
      <div class="inner_main">
          <div class="confirm_title_comment">
              <span class="confirm_title_comment_main">登録内容確認</span><span class="confirm_title_comment_sub">※よろしければ登録ボタンを押して下さい</span>
          </div>
          <div class="table_main_kihon">
            <table>
              <thead>
                <tr class="table_main_kihon_header">
                  <th class="table_main_kihon_header_komoku">登録項目</td>
                  <th class="table_main_kihon_header_naiyo">登録内容</td>
                </tr>
              </thead>
              <tbody>
                <tr class="t_danka">
                  <td id="danka_type">登録区分</td>
                  <script>
                      createHtmlByDankaType("<%= dankaTypeBk %>", "<%= dankaType %>");
                  </script>                    
                </tr>
                <tr class="m_member">
                  <td id="name_type">戸主名(※必須)</td>
                    <script>
                      createHtml("<%= nameFullBk %>", "<%= nameFull %>");
                    </script>
                </tr>
                <tr class="m_member">
                  <td id="furigana_type">ふりがな(※必須)</td>
                    <script>
                      createHtml("<%= furiganaFullBk %>", "<%= furiganaFull %>");
                    </script>
                </tr>
                <tr class="m_address">
                  <td id="zip_code_type">郵便番号</td>
                    <script>
                      createHtml("<%= zipCodeBk %>", "<%= zipCode %>");
                    </script>
                </tr>
                <tr class="m_address">
                  <td id="region_type">都道府県</td>
                    <script>
                      createHtml("<%= regionBk %>", "<%= region %>");
                    </script>
                </tr>
                <tr class="m_address">
                  <td id="city_type">市区町村</td>
                    <script>
                      createHtml("<%= cityBk %>", "<%= city %>");
                    </script>
                </tr>
                <tr class="m_address">
                  <td id="address_line1_type">番地</td>
                    <script>
                      createHtml("<%= addressLine1Bk %>", "<%= addressLine1 %>");
                    </script>
                </tr>
                <tr class="m_address">
                  <td id="address_line2_type">ビル名等</td>
                    <script>
                      createHtml("<%= addressLine2Bk %>", "<%= addressLine2 %>");
                    </script>
                </tr>
                <tr class="m_member">
                  <td id="birthday_type">生年月日</td>
                    <script>
                      createHtml("<%= birthdayBk %>", "<%= birthday %>");
                    </script>
                </tr>
                <tr class="m_telnumber_1">
                  <td id="tel_type">電話番号(優先1)</td>
                    <script>
                      createHtml("<%= telno1Bk %>", "<%= telno1 %>");
                    </script>
                </tr>
                <tr class="m_telnumber_2">
                  <td id="tel_type">電話番号(優先2)</td>
                    <script>
                      createHtml("<%= telno2Bk %>", "<%= telno2 %>");
                    </script>
                </tr>
                <tr class="m_telnumber_3">
                  <td id="tel_type">電話番号(優先3)</td>
                    <script>
                      createHtml("<%= telno3Bk %>", "<%= telno3 %>");
                    </script>
                </tr>
                <tr class="m_mail_1">
                  <td id="email_type">Email(優先1)</td>
                    <script>
                      createHtml("<%= mail1Bk %>", "<%= mail1 %>");
                    </script>
                </tr>
                <tr class="m_mail_2">
                  <td id="email_type">Email(優先2)</td>
                    <script>
                      createHtml("<%= mail2Bk %>", "<%= mail2 %>");
                    </script>
                </tr class="m_mail">
                <tr class="m_mail_3">
                  <td id="email_type">Email(優先3)</td>
                    <script>
                      createHtml("<%= mail3Bk %>", "<%= mail3 %>");
                    </script>
                </tr>
                <tr class="m_member">
                  <td id="job_type">職業</td>
                    <script>
                      createHtml("<%= jobBk %>", "<%= job %>");
                    </script>
                </tr>
                <tr class="t_danka">
                  <td id="tiku_type">地区名</td>
                    <script>
                      createHtml("<%= tikuNameBk %>", "<%= tikuName %>");
                    </script>
                </tr>
                <tr class="t_danka">
                  <td id="sewa_type">世話人</td>
                    <script>
                      createHtml("<%= sewaNameBk %>", "<%= sewaName %>");
                    </script>
                </tr>
                <tr class="t_danka">
                  <td id="jiin_type">所属寺</td>
                    <script>
                      createHtml("<%= jiinNameBk %>", "<%= jiinName %>");
                    </script>
                </tr>
                <tr class="m_member">
                  <td id="tags">タグ</td>
                    <script>
                      createHtml("<%= tagsBk %>", "<%= tags %>");
                    </script>
                </tr>
                <tr class="t_comment">
                  <td class="comment_type">備考<br>※500文字以内</td>
                    <script>
                      createHtml("<%= commentBk %>", "<%= comment %>");
                    </script>
                </tr>
              </tbody>
            </table>
          </div>
          <form action="danka_detail_kihon_db_update" method="post">
            <input type="hidden" name="member_id" value=<%= memberId %>>
            <input type="hidden" name="danka_type" value=<%= dankaType %>>
